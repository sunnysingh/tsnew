{
  "name": "tsnew",
  "version": "0.6.1",
  "description": "Type-safe Code Generator",
  "homepage": "https://tsnew.dev/",
  "author": "Sunny Singh <me@sunnysingh.io> (https://sunnysingh.io/)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sunnysingh/tsnew.git"
  },
  "bugs": {
    "url": "https://github.com/sunnysingh/tsnew/issues"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "bin": {
    "tsnew": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "typescript",
    "scaffold",
    "generator"
  ],
  "peerDependencies": {
    "typescript": "5.x"
  },
  "dependencies": {
    "@clack/prompts": "0.11.0",
    "boxen": "8.0.1",
    "bundle-require": "5.1.0",
    "cac": "6.7.14",
    "colorette": "2.0.20",
    "esbuild": "0.25.5",
    "globby": "14.1.0",
    "oo-ascii-tree": "1.112.0",
    "tree-node-cli": "1.6.0"
  },
  "devDependencies": {
    "@auto-it/all-contributors": "11.3.0",
    "@auto-it/first-time-contributor": "11.3.0",
    "@auto-it/npm": "11.3.0",
    "@auto-it/released": "11.3.0",
    "@types/node": "22.15.29",
    "auto": "11.3.0",
    "npm-check-updates": "18.0.1",
    "prettier": "3.5.3",
    "publint": "0.3.12",
    "tsup": "8.5.0",
    "typescript": "5.8.3"
  },
  "workspaces": [
    "site"
  ],
  "scripts": {
    "bin": "$npm_package_bin_tsnew",
    "dev": "tsup --watch",
    "site": "npm run dev --workspace site",
    "build": "tsup",
    "format": "prettier --write .",
    "formatcheck": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "lint": "publint --strict",
    "coverage": "echo 'Not implemented yet.'; exit 0",
    "test": "echo 'Not implemented yet.'; exit 0",
    "release": "auto shipit",
    "deps:upgrade": "npm-check-updates --interactive --upgrade",
    "deps:outdated": "npm-check-updates"
  }
}
